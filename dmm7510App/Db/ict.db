record(sseq, "$(P)$(R)DownloadSeq1"){
  field(ASG, "Reserved")
  field(DESC, "Download sequence 1")
  field(SELM, "All")
  field(STR1, "ON")
  field(STR2, "DIGITIZE_VOLTAGE")
  field(DO3, "10")
  field(STR4, "DC")
