########################################################################
# Level Threshold
# Desc: Level threshold used by analog trigger.

# Level trigger threshold
record(ao, "$(P)$(R)Threshold-SP"){
  field(DESC, "Threshold level")
  field(DRVH, "40")
  field(DRVL, "0")
  field(PREC, "6")
  field(EGU, "nC")
  field(FLNK, "$(P)$(R)ThreshCalc")
